

namespace "FNT", (exports) ->
  
  class exports.Time
    # All values in milliseconds
    
    @ONE_SECOND:     1000
    @TWO_SECONDS:    2000
    @THREE_SECONDS:  3000
    @FOUR_SECONDS:   4000
    @FIVE_SECONDS:   5000
    @TEN_SECONDS:    10000
    
    # MINUTES:
    @ONE_MINUTE:     60 * @ONE_SECOND
